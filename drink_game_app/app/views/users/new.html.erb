<h1>New Users</h1>



<%= form_for(@user) do |f| %>
  <%= f.label(:name) %>
  <%= f.text_field(:name) %>

  <%= f.label(:email) %>
  <%= f.text_field(:email) %>

   <%= f.label(:age) %>
  <%= f.text_field(:age) %>

  <%= f.label(:gender) %>
  <%= f.text_field(:gender) %>
  <br>
  <%= f.label(:picture_url) %>
  <%= f.text_field(:picture_url) %>


  <%= f.label(:password) %>
  <%= f.password_field(:password) %>

  <%= f.label(:password_confirmation) %>
  <%= f.password_field(:password_confirmation) %>
  <%= f.submit %>

  <% end %>
